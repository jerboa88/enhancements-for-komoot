{
	"compilerOptions": {
		"composite": true,
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
		"skipLibCheck": true,
		"module": "ESNext",

		/* Bundler mode */
		"moduleResolution": "bundler",
		"noEmit": true,

		/* Linting */
		"strict": true,
		"noUncheckedSideEffectImports": true
	},
	"include": ["vite.config.ts"]
}
